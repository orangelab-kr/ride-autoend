{
  "name": "ride-autoend",
  "version": "1.3.4",
  "main": "dist/index.js",
  "repository": "https://github.com/orangelab-kr/ride-autoend.git",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "start:prod": "dotenv -e .env.prod -- node .",
    "start:dev": "dotenv -e .env.dev -- node .",
    "start:stage": "dotenv -e .env.stage -- node .",
    "serve": "yarn clean && dotenv -e .env.dev -- tsc-watch --onSuccess \"node .\"",
    "serve:prod": "yarn clean && dotenv -e .env.prod -- tsc-watch --onSuccess \"node .\"",
    "serve:stage": "yarn clean && dotenv -e .env.stage -- tsc-watch --onSuccess \"node .\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@sentry/node": "^6.14.1",
    "@sentry/tracing": "^6.14.1",
    "@slack/webhook": "^6.0.0",
    "dayjs": "^1.10.7",
    "dotenv-cli": "^4.1.0",
    "firebase-admin": "^10.0.0",
    "iamport": "^0.3.4",
    "liquidjs": "^9.28.5",
    "mongoose": "^6.0.12",
    "mqtt": "^4.2.8",
    "openapi-internal-sdk": "^1.6.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.4.1",
    "@types/request": "^2.48.7",
    "@types/request-promise": "^4.1.48",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "tsc-watch": "4.5.0",
    "typescript": "^4.4.4"
  }
}
